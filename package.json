{
  "name": "prompt-maker",
  "version": "0.1.0",
  "description": "プロンプト生成ツール",
  "main": "index.html",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hideyuki-ogawa/prompt-maker.git"
  },
  "keywords": [
    "prompt",
    "generator",
    "tool"
  ],
  "author": "hideyuki-ogawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hideyuki-ogawa/prompt-maker/issues"
  },
  "homepage": "https://github.com/hideyuki-ogawa/prompt-maker#readme",
  "devDependencies": {
    "@playwright/test": "^1.52.0",
    "@types/jest": "^29.5.12",
    "http-server": "^14.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "collectCoverageFrom": [
      "script.module.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}
